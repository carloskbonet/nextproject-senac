// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = "file:./dev.db"
}

model user {
  id       Int     @id @unique @default(autoincrement())
  name     String?
  cpf      String  @unique
  email    String  @unique
  password String
}